      <script>
        document.getElementById('buscar').addEventListener('keydown', function(event) {
          if (event.key === 'Enter') {
            event.preventDefault();
            buscarLibros();
          }
        });
      
        document.getElementById('buscador').addEventListener('click', function(event) {
          buscarLibros();
        });
      
        function buscarLibros() {
          const query = document.getElementById('buscar').value;
          window.location.href = '/buscar?query=' + query;
        }
      </script>

